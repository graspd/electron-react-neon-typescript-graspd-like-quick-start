{
  "name": "electron-react-neon-typescript-graspd-like-quick-start",
  "version": "1.0.0",
  "description": "Borderplate for creating app with Electron + React + Neon Bindings and TypeScript.",
  "main": "dist/main/index.js",
  "private": true,
  "scripts": {
    "build-dev": "electron-build-env neon build native-addon && webpack --config webpack.config.ts --mode development",
    "build": "electron-build-env neon build native-addon --release && webpack --config webpack.config.ts",
    "start": "electron .",
    "build-and-start": "npm run build && npm run start",
    "build-and-start-dev": "npm run build-dev && npm run start",
    "scan-dependencies": "ncu && cd src/native-addon && ncu",
    "update-dependencies": "ncu -u  && cd src/native-addon && ncu -u",
    "package-mac": "electron-packager . --overwrite --ignore=\"node_modules\" --ignore=\"src\" --ignore=\".gitignore\" --ignore=\"README.md\" --ignore=\"package-lock.json\" --ignore=\"webpack.config.ts\" --ignore=\"tsconfig.json\" --ignore=\"release-builds\" --ignore=\"audit-ci.json\" --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "package-win-64": "electron-packager . --overwrite --ignore=\"node_modules\" --ignore=\"src\" --ignore=\".gitignore\" --ignore=\"README.md\" --ignore=\"package-lock.json\" --ignore=\"webpack.config.ts\" --ignore=\"tsconfig.json\" --ignore=\"release-builds\" --ignore=\"audit-ci.json\" --asar --platform=win32 --arch=x64 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"ExampleApp\"",
    "package-win-32": "electron-packager . --overwrite --ignore=\"node_modules\" --ignore=\"src\" --ignore=\".gitignore\" --ignore=\"README.md\" --ignore=\"package-lock.json\" --ignore=\"webpack.config.ts\" --ignore=\"tsconfig.json\" --ignore=\"release-builds\" --ignore=\"audit-ci.json\" --asar --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"ExampleApp\"",
    "package-linux": "electron-packager . --overwrite --ignore=\"node_modules\" --ignore=\"src\" --ignore=\".gitignore\" --ignore=\"README.md\" --ignore=\"package-lock.json\" --ignore=\"webpack.config.ts\" --ignore=\"tsconfig.json\" --ignore=\"release-builds\" --ignore=\"audit-ci.json\" --asar --platform=linux --arch=x64 --prune=true --out=release-builds",
    "audit-dependencies": "audit-ci --config audit-ci.json",
    "depcheck": "depcheck",
    "lint": "eslint -f unix \"src/**/*.{ts,tsx}\" && prettier --write \"src/**/*.{ts,tsx}\""
  },
  "repository": "git+https://github.com/graspd/electron-react-neon-typescript-graspd-like-quick-start.git",
  "keywords": [
    "electron"
  ],
  "author": "Krzysztof Zawis≈Ça, Aurelia Korbela",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/graspd/electron-react-neon-typescript-graspd-like-quick-start/issues"
  },
  "homepage": "https://github.com/graspd/electron-react-neon-typescript-graspd-like-quick-start#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "devDependencies": {
    "@microsoft/tsdoc": "^0.12.24",
    "@swc/cli": "^0.1.27",
    "@swc/core": "^1.2.40",
    "@swc/helpers": "^0.2.4",
    "@types/case-sensitive-paths-webpack-plugin": "^2.1.4",
    "@types/node": "^14.14.12",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/source-map-support": "^0.5.3",
    "@types/terser-webpack-plugin": "^5.0.2",
    "@types/webpack-bundle-analyzer": "^3.9.0",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "audit-ci": "^3.1.1",
    "better-docs": "^2.3.2",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "depcheck": "^1.3.1",
    "electron": "^11.0.4",
    "electron-build-env": "^0.2.0",
    "electron-packager": "^15.2.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsdoc": "^30.7.8",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-tsdoc": "^0.2.10",
    "eslint-plugin-unicorn": "^23.0.0",
    "eslint-webpack-plugin": "^2.4.1",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.5",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.6",
    "lighthouse": "^6.5.0",
    "neon-cli": "^0.5.3",
    "node-loader": "^1.0.2",
    "npm-check-updates": "^10.2.5",
    "prettier": "^2.2.1",
    "source-map-loader": "^1.1.3",
    "spectron": "^13.0.0",
    "swc-loader": "^0.1.12",
    "terser-webpack-plugin": "^5.0.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2",
    "webpack": "^5.10.0",
    "webpack-bundle-analyzer": "^4.2.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "electron-is-dev": "^1.2.0",
    "native-addon": "file:src/native-addon",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet-async": "^1.0.7",
    "react-router-dom": "^5.2.0",
    "source-map-support": "^0.5.19",
    "styled-components": "^5.2.1"
  }
}
